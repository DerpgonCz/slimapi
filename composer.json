{
    "name": "slimapi/slimapi",
    "description": "SlimAPI",
    "license": "MIT",
    "archive": {
        "exclude": [".*", "*", "!/src", "!LICENSE.md", "!README.md"]
    },
    "autoload": {
        "psr-4": {
            "SlimAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlimAPI\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "nette/bootstrap": "^3.0"
    },
    "require-dev": {
        "tracy/tracy": "~2.7.5"
    }
}
